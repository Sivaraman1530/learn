{
    "version": 2,
    "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "src/**/*.ts",
        "outputDirectory": "service"
      }
    }
  ],
    "rewrites": [
        {
            "source":  "/(.*)",
            "destination": ".src/index.ts"
        }
    ]
}